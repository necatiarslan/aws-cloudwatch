{
  "name": "aws-cloudwatch-vscode-extension",
  "displayName": "Aws CloudWatch",
  "description": "Browse Your AWS CloudWatch Logs",
  "version": "1.0.1",
  "publisher": "NecatiARSLAN",
  "repository": {
    "type": "git",
    "url": "https://github.com/necatiarslan/aws-cloudwatch"
  },
  "icon": "media/aws-cloudwatch-logo.png",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:aws-cloudwatch-vscode-extension"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aws-cloudwatch-vscode-extension.CheckAccessibility",
        "title": "Aws CloudWatch:Check Accessibility"
      },
      {
				"command": "aws-cloudwatch-vscode-extension.Filter",
				"title": "Filter",
				"icon": "$(filter)"
			},
      {
				"command": "aws-cloudwatch-vscode-extension.ShowOnlyFavorite",
				"title": "Show Only Favorite",
				"icon": "$(bookmark)"
			},
      {
				"command": "aws-cloudwatch-vscode-extension.AddToFav",
				"title": "Fav"
			},
			{
				"command": "aws-cloudwatch-vscode-extension.DeleteFromFav",
				"title": "UnFav"
			}
    ],
    "viewsContainers": {
			"activitybar": [
				{
					"id": "aws-cloudwatch-vscode-extension",
					"title": "Aws Cloud Watch",
					"icon": "media/aws-cloudwatch-logo.svg"
				}
			]
		},
		"views": {
			"aws-cloudwatch-vscode-extension": [
				{
					"id": "aws-cloudwatch-vscode-extension.TreeView",
					"name": "Aws Cloud Watch",
					"icon": "media/aws-cloudwatch-logo.svg"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "aws-cloudwatch-vscode-extension.TreeView",
				"contents": "Can Not Connect to Aws Cloud Watch"
			}
		]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.3",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@aws-sdk/credential-provider-node": "^3.181.0"
  }
}
